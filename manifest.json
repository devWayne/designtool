{
    "manifest_version": 2,

    "name": "fehelper",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": ["zepto-v1.1.6.min.js"],
    "permissions": ["<all_urls>"]
}
